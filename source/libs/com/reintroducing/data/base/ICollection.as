package com.reintroducing.data.base{	/**	 * Interface for Collection.	 * 	 * @author Matt Przybylski [http://www.reintroducing.com]	 * @version 1.0	 */	public interface ICollection	{		function getName():String;		function addItem($item:*):Boolean;		function addItems(...$items):void;		function addItemAt($index:int, $item:*):void;		function removeItem($item:*):Boolean;		function clear():void;		function contains($item:*):Boolean;		function getItemAt($index:int):*;		function getIterator():Iterator;		function getLength():int;		function isEmpty():Boolean;		function output():void;		//- END INTERFACE -----------------------------------------------------------------------------------------	}}